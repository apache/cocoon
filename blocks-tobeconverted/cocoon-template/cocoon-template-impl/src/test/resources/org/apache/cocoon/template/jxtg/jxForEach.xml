<?xml version="1.0"?>

<root xmlns:jx="http://apache.org/cocoon/templates/jx/1.0">
  <jx:forEach var="i" items="${test}">
    <forEach>${i}</forEach>
  </jx:forEach>
  
  <loop>
    <jx:forEach varStatus="status" begin="1" end="5">
      <forEach>${status.index}</forEach>
    </jx:forEach>
  </loop>
</root>
