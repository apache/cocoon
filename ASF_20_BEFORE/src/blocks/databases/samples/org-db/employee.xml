<?xml version="1.0"?>

<employee>
  <connection>personnel</connection>
  <table name="employee">
    <keys>
      <key param="employee" dbcol="id" type="int" mode="manual"/>
    </keys>
    <values>
      <value param="name" dbcol="name" type="string"/>
      <value param="department" dbcol="department_id" type="int"/>
    </values>
  </table>
</employee>
