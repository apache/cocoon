<?xml version="1.0"?>
<validator>
  <datatype>
    <name>Literal</name>
    <expression>[A-Za-z0-9/.\\\-;:_ ()=*+]+</expression>
  </datatype>
  <datatype>
    <name>Dimension</name>
    <expression>[0-9,.]+x[0-9,.]+</expression>
  </datatype>
  <datatype>
    <name>Number</name>
    <expression>[0-9]+</expression>
  </datatype>
  <datatype>
    <name>Boolean</name>
    <expression>(Yes)|(No)</expression>
  </datatype>
</validator>
